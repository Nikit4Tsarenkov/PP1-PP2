//A4(4)
#include<iostream>
using namespace std;

int main()
{
	float f, x;
	cout << "Input arg x:"; cin >> x;
	if (x <= 1)
		f = 0;
	else
		f = 1 / (x + 6);
	cout << "f(" << x << ")=" << f << endl;
	system("pause");
	return 0;
}


//A5(5)
#include<iostream>
using namespace std;

int main()
{
	int y, x, R=3;
	cout << "Input arg x:="; cin >> x;
	y = -3; 
	if (x >= -8 && x <= -5);
	y = x + 3;
	if (x >= 5 && x <= -3);
	y = sqrt(pow(x, 2) - (x*x));
	if (x > -3 && x <= 3);
	y = ((3*x / 5) + 2);
	if (x > 3 && x <= 8);
	y = 3;
	if (x > 8 && x <= 10);
	cout << "f(" << x << ")=" << y;
	system("pause");
	return 0;
}

