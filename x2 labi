//B5
#include <iostream>
#include <ctime>

using namespace std;

int main()
{
	srand(unsigned(time(0)));
	const int n = 10;
	int a[n];
	int i,sum=0,j;
	double average;


	for (i = 0; i < n; i++)
	{
		a[i] = rand() % 20 + 1;
	cout << a[i] << ' ';
	}
	cout << endl;
	int max = a[0], min = a[0];
	int numMin, numMax;
	for (i = 0; i < n; i++)
	{
		if (a[i] >= max)
		{
			max = a[i];
			 numMax = i;
		}

		if (a[i] <= min)
		{
			min = a[i];
			 numMin = i;
		}
	}
	
	cout << "Index Min = "<< numMin << "\n"<<"Index Max = " << numMax << endl;
	
	if (numMin < numMax)
	{
		for (j=numMin + 1; j < numMax; j++)
			sum += a[j];
		average = (double)sum / (numMax - numMin-1);
	}
	if (numMin > numMax)
	{
		for (j=numMax + 1; j < numMin; j++)
			sum += a[j];
		average =(double) sum / (numMin - numMax-1);
	}

	if (abs(numMin - numMax)==1)
		average = 0;
	
	cout << "Average between " << a[numMin] << " and " << a[numMax] << " = " << average << endl;

	

	system("pause");
	return 0;
}

