#include <iostream> 
#include <string> 
using namespace std;
int main() {
	cout << " Text: ";

	string text = "";

	char ch = 0;

	for (int n = 0; ch != '.'; n++)

	{
		cin.get(ch);
		text += ch;
	}

	int digits[10] = { 0 };

	for (int n = 0; text[n] != '.'; n++) ++digits[text[n] - 48];
	int max = 0;

	for (int n = 0; n < 10; ++n)
	{
		if (max < digits[n]) 
		{
			max = digits[n];
			ch = n + 48;
		}
	}
	cout << " Digit: " << ch << "; max: " << max << endl;
	
	system("pause");
	
	return 0;
}*/


