//B2
#include <iostream>
#include <ctime>
#include <iomanip>
using namespace std;
 
int main()
{
	const int n = 10;
	int a[n],i,sum=0,average,k=0;
	srand(unsigned(time(0)));
	for (i = 0; i < n; i++)
		a[i] = rand() % 10+1 ;
	cout << "Sourse array: ";
	for (i = 0; i < n; i++) 
	{
		sum += a[i];
		cout << a[i]<<' ';
	}
	average = sum / n; cout <<"\nAverage = " <<average<<endl;
	for (i = 0; i < n; i++)
	{
		a[k++] = a[i] - average;
		 cout << a[i] << ' ';
	}

	

	system("pause");
	return 0;
	




}

//B3
#include <iostream>
#include <ctime>

using namespace std;

int main()
{
	srand(unsigned(time(0)));
	const int n = 10; int a[n];
	 int l = n;
	int i, k=0,j=0,imin;
	cout << "Sourse array: ";
	for (i = 0; i < n; i++)
	{
		a[i] = rand() % 10 + 1;
		cout << a[i] << ' ';
	}
	cout << endl;
	for (i = 0; i < n/2; i++)
	{
		int tmp = a[i];
		a[i] = a[n - i - 1];
		a[n - i - 1] = tmp;
	}

	for (i = 0; i < n;i++)
		cout << a[i] << ' ';
	
	system("pause");
	return 0;
}

